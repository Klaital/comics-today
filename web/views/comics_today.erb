<html>
<head>
  <title>Today's Comics</title>
</head>
<body>
  <div id="header">
    <h1>Today's Comics</h1>
  </div>
  <div id="leftnav">
    <ul class="comics-list">
    <% comics.each do |c| %>
      <li><a href="<%= c['url'] %>"><%= c['name'] %></a></li>
    <% end %>
    </ul>
  </div>
</body>
</html>
